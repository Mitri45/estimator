{
    "compilerOptions": {
        "strict": true,
        "module": "NodeNext",
        "strictNullChecks": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "jsx": "react-jsx",
        "moduleResolution": "NodeNext",
        "forceConsistentCasingInFileNames": true,
    }
}